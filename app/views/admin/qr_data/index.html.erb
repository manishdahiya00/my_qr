<!-- Page Content -->
<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h2 class="page-header" style="margin: 10px 0 15px !important;">Qr Data</h2>
      </div>
      <% if @qrData.present? %>
        <table class="table table-responsive table-bordered">
          <thead>
            <tr>
              <th>Id</th>
              <th>Code Data</th>
              <th>Device Id</th>
              <th>Created At</th>
              <th>Updated At</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% @qrData.each do |qr| %>
              <tr>
                <td><%= qr.id %></td>
                <td><%= qr.codeData[0,60] + "..." %></td>
                <td>Device Detail <%= qr.device_detail_id %></td>
                <td><%= qr.created_at %></td>
                <td><%= qr.updated_at %></td>
                <td>
                  <%= link_to 'Show', admin_qr_datum_path(qr), class: 'btn btn-primary' %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
        <%= will_paginate @qrData %>
      <% else %>
        <h1 class="not-found">No Qr Data Found</h1>
      <% end %>
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
