<!-- Page Content -->
<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <h2 class="page-header" style="margin: 10px 0 15px !important;">Device Details</h2>
      </div>
      <% if @deviceDetails.present? %>
        <table class="table table-responsive table-bordered">
          <thead>
            <tr>
              <th>Id</th>
              <th>Device Name</th>
              <th>Device Id</th>
              <th>Security Token</th>
              <th>Advertising Id</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% @deviceDetails.each do |device| %>
            <tr>
              <td><%= device.id %></td>
              <td><%= device.deviceName %></td>
              <td><%= device.deviceId %></td>
              <td><%= device.security_token %></td>
              <td><%= device.advertisingId %></td>
              <td>
                <%= link_to 'Show', admin_device_detail_path(device), class: 'btn btn-primary' %>
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>
        <%= will_paginate @deviceDetails %>
      <% else %>
        <h1 class="not-found">No Device Found</h1>
      <% end %>
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
